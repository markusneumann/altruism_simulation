@article{Santos2005,
abstract = {We study the evolution of cooperation in the framework of evolutionary game theory, adopting the$\backslash$r$\backslash$nprisoner's dilemma and snowdrift game as metaphors of cooperation between unrelated individuals. In$\backslash$r$\backslash$nsharp contrast with previous results we find that, whenever individuals interact following networks of$\backslash$r$\backslash$ncontacts generated via growth and preferential attachment, leading to strong correlations between$\backslash$r$\backslash$nindividuals, cooperation becomes the dominating trait throughout the entire range of parameters of$\backslash$r$\backslash$nboth$\backslash$r$\backslash$ngames, as such providing a unifying framework for the emergence of cooperation. Such emergence is$\backslash$r$\backslash$nshown to be inhibited whenever the correlations between individuals are decreased or removed. These$\backslash$r$\backslash$nresults are shown to apply from very large population sizes down to small communities with nearly$\backslash$r$\backslash$n100 individuals.},
author = {Santos, F. C. and Pacheco, J. M.},
doi = {10.1103/PhysRevLett.95.098104},
file = {:media/mneumann/ec574740-a4f4-4bd0-b624-6ff2c4ac59e9/Dropbox/Dropbox/Paper{\_}Altruism/literature/PhysRevLett.95.098104.pdf:pdf},
