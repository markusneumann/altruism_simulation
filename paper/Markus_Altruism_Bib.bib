title = {{Social Capital and Politics}},
volume = {1},
year = {1998}
}
@article{Cremene2014,
abstract = {The punishment effect on social behavior is analyzed within the strategic interaction framework of Cellular Automata and computational Evolutionary Game Theory. A new game, called Social Honesty (SH), is proposed. The SH game is analyzed in spatial configurations. Probabilistic punishment is used as a dishonesty deterrence mechanism. In order to capture the intrinsic uncertainty of social environments, payoffs are described as random variables. New dynamics, with a new relation between punishment probability and punishment severity, are revealed. Punishment probability proves to be more important than punishment severity in guiding convergence towards honesty as predominant behavior. This result is confirmed by empirical evidence and reported experiments. Critical values and transition intervals for punishment probability and severity are identified and analyzed. Clusters of honest or dishonest players emerge spontaneously from the very first rounds of interaction and are determinant for the future dynamics and outcomes.},
author = {Cremene, Marcel and Dumitrescu, D. and Cremene, Ligia},
doi = {10.1371/journal.pone.0087471},
file = {:media/mneumann/ec574740-a4f4-4bd0-b624-6ff2c4ac59e9/Dropbox/Dropbox/Paper{\_}Altruism/literature/cremene2014.pdf:pdf},
isbn = {1932-6203},
issn = {19326203},
journal = {PLoS ONE},
number = {1},
pages = {1--10},
pmid = {24489917},
title = {{A strategic interaction model of punishment favoring contagion of honest behavior}},
volume = {9},
year = {2014}
}
@article{Fowler2007,
abstract = {Scholars have recently extended the traditional calculus of participation model by adding a term for benefits to others. We advance this work by distinguishing theoretically a concern for others in general (altruism) from a concern for others in certain groups (social identification). We posit that both concerns generate increased benefits from participation. To test these theories, we use allocations in dictator games towards an unidentified anonymous recipient and two recipients identified only as a registered Democrat or a registered Republican. These allocations permit a distinction between altruism and social identification. The results show that both altruism and social identification significantly increase political participation. The results also demonstrate the usefulness of incorporating benefits that stem from sources beyond material self-interest into rational choice models of participation. {\textcopyright} 2007 Southern Political Science Association.},
author = {Fowler, James H. and Kam, Cindy D.},
doi = {10.1111/j.1468-2508.2007.00577.x},
file = {:home/mneumann/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Fowler, Kam - 2007 - Beyond the self Social identity, altruism, and political participation.pdf:pdf},
isbn = {0022-3816},
issn = {00223816 (ISSN)},
journal = {Journal of Politics},
number = {3},
pages = {813--827},
title = {{Beyond the self: Social identity, altruism, and political participation}},
